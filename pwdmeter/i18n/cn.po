#: pwdmeter/factors/length.py:27
msgid "Increase the length of the password"
msgstr "增加密码长度"

#: pwdmeter/factors/variety.py:28 pwdmeter/factors/variety.py:30
msgid "Minimize character duplicates and repetitions"
msgstr "减少字符重复"

#: pwdmeter/factors/charmix.py:32
msgid "Use a good mix of numbers, letters, and symbols"
msgstr "混合使用数字,字母和其他字符"

#: pwdmeter/factors/non_ascii.py:25
msgid "Use non-ASCII chars"
msgstr "使用非英文字符"

#: pwdmeter/factors/casemix.py:27 pwdmeter/factors/casemix.py:29
msgid "Use a good mix of upper case and lower case letters"
msgstr "混合使用大写和小写字符"

#: pwdmeter/factors/non_dictionary.py:20
msgid "Avoid using most common passwords"
msgstr "避免使用常用密码"
